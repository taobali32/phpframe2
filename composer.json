{
  "name": "jtar/socket",
  "type": "library",
  "description": "The socket library.",
  "keywords": [
    "jtar"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "opis/closure": "3.6.1",
    "ext-pcntl": "*",
    "ext-event": "*",
    "ext-sockets": "*",
    "ext-posix": "*",
    "ext-sysvmsg": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Jtar\\": "src/",
      "App\\": "app/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}